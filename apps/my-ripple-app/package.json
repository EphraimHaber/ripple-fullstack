{
  "name": "my-ripple-app",
  "version": "1.0.0",
  "description": "A Ripple application created with create-ripple",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "serve:dev": "vite preview --mode development",
    "serve:prod": "vite preview --mode production",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "preview": "pnpm preview:dev",
    "preview:prod": "pnpm build:prod && pnpm serve:prod",
    "preview:dev": "pnpm build:dev && pnpm serve:dev"
  },
  "license": "MIT",
  "devDependencies": {
    "@ripple-ts/eslint-plugin": "latest",
    "@ripple-ts/prettier-plugin": "latest",
    "@ripple-ts/typescript-plugin": "latest",
    "@ripple-ts/vite-plugin": "latest",
    "@tailwindcss/vite": "^4.1.17",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "ripple": "latest",
    "ripple-router": "^0.4.10",
    "tanstack-ripple-query": "^0.0.3"
  }
}
